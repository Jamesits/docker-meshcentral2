{
  "$schema": "http://info.meshcentral.com/downloads/meshcentral-config-schema.json",
  "settings": {
    "WANonly": false,
    "LANonly": false,
    "_sessionKey": "MyReallySecretPassword1",
    "port": 443,
    "aliasPort": 443,
    "redirPort": 80,
    "redirAliasPort": 80,
    "mongoDb": "mongodb://meshcentral:hunter2@mongo:27017/meshcentral",
    "mongoDumpPath": "/usr/bin/mongodump",
    "allowLoginToken": true,
    "allowFraming": false,
    "cookieIpCheck": false,
    "compression": true,
    "webRTC": true,
    "nice404": true,
    "clickOnce": true,
    "selfUpdate": false,
    "allowHighQualityDesktop": true,
    "desktopMultiplex": true,
    "no2FactorAuth": false,
    "watchDog": { "interval": 100, "timeout": 400 },
    "autoBackup": { "backupIntervalHours": 24, "keepLastDaysBackup": 10, "zipPassword": "supers3cret", "backupPath": "/srv/meshcentral2/backup" }
  },
  "domains": {
    "": {
      "_title": "MyServer",
      "_title2": "Servername",
      "_minify": true,
      "_newAccounts": true,
      "_userNameIsEmail": true
    }
  },
  "_letsencrypt": {
    "__comment__": "Requires NodeJS 8.x or better, Go to https://letsdebug.net/ first before trying Let's Encrypt.",
    "email": "myemail@mydomain.com",
    "names": "myserver.mydomain.com",
    "production": false
  }
}

